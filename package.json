{
  "name": "moefile",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "./scripts/postinstall:clean.sh && bun run tmplgen -dev && go get ./cmd/*/",
    "clean": "rm -rf ./node_modules/ && ./scripts/postinstall:clean.sh",
    "tmplgen": "go run ./cmd/tmplgen/",
    "dev": "bun run build:frontend && bun run dev:backend",
    "dev:frontend": "./scripts/dev:frontend.sh",
    "dev:backend": "go run ./cmd/moefile",
    "build": "bun run build:frontend && make all",
    "build:frontend": "bun run tmplgen -prod && ./scripts/build:frontend.sh",
    "build:backend": "bun run build:frontend && make",
    "internal:tsc": "tsc -b",
    "internal:vite": "vite"
  },
  "dependencies": {
    "@radix-ui/react-aspect-ratio": "^1.1.1",
    "@radix-ui/react-context-menu": "^2.2.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.4",
    "artplayer": "^5.2.1",
    "artplayer-plugin-danmuku": "^5.1.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^24.1.2",
    "i18next-browser-languagedetector": "^8.0.2",
    "lucide-react": "^0.468.0",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.2.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "globals": "^15.12.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1",
    "vite-plugin-singlefile": "^2.1.0"
  }
}
